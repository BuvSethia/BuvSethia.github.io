{
  "name": "BuvSethia.github.io",
  "description": "Personal website",
  "author": "Sumbhav Sethia",
  "scripts": {
    "start": "npm-run-all --parallel lint:watch webpackserver",
    "webpackserver": "webpack-dev-server --config webpack.config.dev.js --content-base src/ --hot --inline --open",
    "lint": "node_modules/.bin/esw webpack.config.* ./src/",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "webpack:prod": "webpack --config webpack.config.prod.js",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "npm-run-all webpack:prod"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "bootstrap": "^3.3.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.16.3",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.8.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^3.1.1",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
